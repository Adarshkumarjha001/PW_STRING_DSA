#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    string str="raghav is a math teacher. He is a dsa mentor also";
    stringstream ss(str);
    string temp;
    vector<string> v;
    while(ss>>temp)
    {
        v.push_back(temp);
    }
    sort(v.begin(),v.end());
    int maxCount=1;
    int count=1;
    for(int i=1;i<v.size();i++){
        if(v[i]==v[i-1]) count++;
        else count=1;
        maxCount=max(maxCount,count);
            
        
    }
    count=1;
    for(int i=1;i<v.size();i++){
        if(v[i]==v[i-1]) count++;
        else count=1;
        if(maxCount==count){
        cout<<v[i]<<"  "<<maxCount<<endl;
    }
    }
      return 0;
}

#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
    string str="raghav is a math teacher. He is a dsa mentor also";
    stringstream ss(str);
    string temp;
    vector<string> v;
    while(ss>>temp)
    {
        v.push_back(temp);
    }
    sort(v.begin(),v.end());
    int maxCount=1;
    int count=1;
    for(int i=1;i<v.size();i++){
        if(v[i]==v[i-1]) count++;
        else count=1;
        maxCount=max(maxCount,count);
            
        
    }
    count=1;
    for(int i=1;i<v.size();i++){
        if(v[i]==v[i-1]) count++;
        else count=1;
        if(maxCount==count){
        cout<<v[i]<<"  "<<maxCount<<endl;
    }
    }
        

    return 0;
}

#include<bits/stdc++.h>
using namespace std;
int main(){

// STOI && STOLL
    string s="123456";
    int x=stoi(s);
    cout<<x<<endl;
    int a=1234567;
    string p=to_string(a);
    cout<<p<<endl;
}

string s="1234563457545666754";
    long long x=stoll(s);
    cout<<x<<endl;
    int a=1234567;
    string p=to_string(a);
    cout<<p<<endl;
 return 0;

