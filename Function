#include<iostream>
using namespace std;
int main(){
	int n;
	cin>>n;
	int arr[100];
	bool counted[100]={false};
	for(int i=0;i<n;i++){
		cin>>arr[i];
	}
	cout<<"Element"<<" "<<"Frequency"<<endl;
	for(int i=0;i<n;i++){
		int count=0;
		if(!counted[i]){
			for(int j=i;j<n;j++){
				if(arr[i]==arr[j]){
					count++;
					counted[j]=true;
				}
			}
		
			
			cout<<arr[i]<<"           "<<count<<endl;
		}
		}
	}
	
