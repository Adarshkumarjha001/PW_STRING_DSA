//FACTORIAL FROM 1 to n
#include<iostream>
#include<cmath>
using namespace std;
int main(){
	int n;
	cin>>n;
	int fact=1;
	for(int i=1;i<=n;i++){
//		int fact=1;
		fact=fact*i;
		cout<<fact<<" ";
	}
}

